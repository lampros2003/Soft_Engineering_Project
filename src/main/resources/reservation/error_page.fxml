<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="160" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.reservation.ErrorPageController">

    <!-- message -->
    <Label fx:id="messageLabel" layoutX="20" layoutY="30" prefWidth="280" style="-fx-font-size: 14;" wrapText="true" />

    <!-- Back button -->
    <Button fx:id="backButton" layoutX="140.0" layoutY="121.0" onAction="#onBackClick" text="Back" />
</AnchorPane>
